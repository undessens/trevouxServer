{
  "name": "radiologicserver",
  "version": "1.0.0",
  "description": "server linking radiologic components together",
  "main": "dist/server.js",
  "directories": {
    "lib": "src"
  },
  "scripts": {
    "start": "node --inspect=0.0.0.0:9222 -r tsconfig-paths/register -r ts-node/register ./src/server.ts",
    "start:watch": "nodemon",
    "build": "tsc -p . ",
    "run": "node dist/server.js",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "author": "UnDesSens",
  "license": "ISC",
  "dependencies": {
    "express": "^4.17.1",
    "ip": "^1.1.5",
    "osc": "^2.4.0",
    "tslib": "^1.11.0"
  },
  "nodemonConfig": {
    "ignore": [
      "**/*.test.ts",
      "**/*.spec.ts",
      ".git",
      "node_modules"
    ],
    "watch": [
      "src",
      "public"
    ],
    "exec": "npm start",
    "ext": "ts"
  },
  "devDependencies": {
    "@types/express": "^4.17.2",
    "@types/node": "^13.7.4",
    "@types/socket.io": "^2.1.4",
    "ts-node": "^8.6.2",
    "tsconfig-paths": "^3.9.0",
    "typescript": "^3.8.2"
  }
}
